{
  "name": "universal-javascript-vue",
  "version": "1.2.9",
  "description": "A fully-isomorphic (universal) ES6/ES7 boilerplate based on Vue.js",
  "author": "Justin Helmer",
  "license": "MIT",
  "homepage": "https://github.com/justinhelmer/universal-javascript-vue#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/justinhelmer/universal-javascript-vue.git"
  },
  "bugs": {
    "url": "https://github.com/justinhelmer/universal-javascript-vue/issues"
  },
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config config/webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config config/webpack.server.config.js --progress --hide-modules",
    "db": "mongod",
    "db:clean": "sudo rm -rf /data/db && sudo mkdir /data/db && sudo chmod -R 777 /data/db",
    "dev": "node bin/start-all-servers",
    "start": "cross-env NODE_ENV=production node server"
  },
  "keywords": [
    "universal",
    "isomorphic",
    "javascript",
    "babel",
    "es6",
    "ssr",
    "server side rendering",
    "vue"
  ],
  "dependencies": {
    "axios": "^0.16.2",
    "body-parser": "^1.17.2",
    "chalk": "^2.0.1",
    "compression": "^1.7.0",
    "cross-env": "^5.0.4",
    "express": "^4.15.3",
    "keystone": "^4.0.0-beta.5",
    "lru-cache": "^4.1.1",
    "mongodb": "^2.2.30",
    "vue": "^2.4.2",
    "vue-router": "^2.7.0",
    "vue-server-renderer": "^2.4.2",
    "vuex": "^2.3.1",
    "vuex-router-sync": "^4.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.6.0",
    "css-loader": "^0.28.4",
    "cssnano": "^3.10.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "font-awesome": "^4.7.0",
    "forever-monitor": "^1.7.1",
    "foundation-sites": "^6.4.1",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "json-server": "^0.12.0",
    "lodash": "^4.17.4",
    "memory-fs": "^0.4.1",
    "motion-ui": "^1.2.3",
    "node-sass": "^4.5.3",
    "postcss-cssnext": "^3.0.2",
    "postcss-loader": "^2.0.6",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "url-loader": "^0.5.9",
    "vue-loader": "^13.0.4",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.4.2",
    "webpack": "^3.4.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-merge": "^4.1.0",
    "webpack-node-externals": "^1.6.0"
  }
}
